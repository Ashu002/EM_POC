{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "whitespace": [
            false

        ],
        "no-trailing-whitespace": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "object-literal-sort-keys": [
            false
        ],
        "trailing-comma": [
            false
        ], 
        "one-variable-per-declaration": [
            false
        ]
    },
    "rulesDirectory": []
}